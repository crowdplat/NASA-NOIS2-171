{
    "name": "RR3 example",
    "short_name": "rr3",
    "experiment_type": "multimodal", 

    "verbose": 1,
    "causalnex_frm": 1, 
    "test_val_split": [0.3, 0.2],
    "data_options": {
        "dataset_fp": "data/rr3_dataset/merged_features.pkl",
        "subject_keys": "sample",
        "targets": ["label"],
        "predictors": "All",
        "environments": ["env_split"],
        "exclude": ["sample", "image_name", "image_label", "env_split"],
	"output_data_regime": "binary"
    },
    "feature_selection_options": {
        "max_features": 1000,
        "verbose": 0,
        "seed": 100
    },
    "ensemble_options": {
        "models": ["RF"]
    },
    "image_data": {
        "image_dir": "data/rr3_dataset/preprocessed_images/",
        "labels_csv": "data/rr3_dataset/csvs/rr3_image_dataset_labels.csv",
        "model_type": "DenseNet121",
        "augmentation": false, 
        "batch_size": 16,
        "learning_rate": 0.0001,
        "num_epochs": 500,
        "split_ratio": 0.8,
        "model_save_path": "image_model_saved/image_model_rr3.pth",
        "image_features_save_path": "data/rr3_dataset/image_features.pkl",
        "tabular_features_path": "data/rr3_dataset/GLDS-352_rna_seq_Normalized_Counts.pickle"
    }
}
