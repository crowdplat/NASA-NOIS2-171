{
    "name": "RR3 example",
    "short_name": "rr3",
    "experiment_type": "multimodal", 

    "verbose": 1,
    "causalnex_frm": 1, 
    "test_val_split": [0.2, 0.2],
    "data_options": {
        "dataset_fp": "data/rr3_dataset/merged_features.pkl",
        "subject_keys": "sample",
        "targets": ["label"],
        "predictors": "All",
        "environments": ["env_split"],
        "exclude": ["sample", "image_name", "image_label", "env_split"],
	"output_data_regime": "binary"
    },
    "feature_selection_options": {
        "max_features": 20,
        "verbose": 0,
        "seed": 12
    },
    "ensemble_options": {
        
        "models": ["ERM", "RF", "ICP", "NLICP"]
    },
    
    "image_data": {
        "image_dir": "data/rr3_dataset/preprocessed_images_2/",
        "labels_csv": "data/rr3_dataset/csvs/rr3_image_dataset_labels.csv",
        "model_type": "CNN_Scratch",
        "image_model_training_type": "train_test_split", 
        "split_ratio": 0.85,
        "augmentation": true, 
        "batch_size": 32,
        "learning_rate": 0.00001,
        "num_epochs": 500,
        "model_save_path": "image_model_saved/image_model_rr3.pth",
        "gradcam_features_save_path": "data/rr3_dataset/gradcam_features.pkl",
        "tabular_features_path": "data/rr3_dataset/GLDS-352_rna_seq_Normalized_Counts.pickle",

        "image_model_gradcam": {
            "apply_gradcam": true,
            "gradcam_output_save_path": "data/rr3_dataset/gradcam_outputs"
        }
        
    }
}
